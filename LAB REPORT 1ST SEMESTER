scanf("%d",&a); printf("Enter B="); scanf("%d",&b); printf("Enter C="); scanf("%d",&c);\ sum=a+b+c;
printf("Total Sum=%d\n",sum); avg=sum/3;
printf("Total Average=%f\n",avg); return 0;
}

